{
	"name": "therealfoobar/openlol-api",
	"description": "API for Openlol",
	"homepage": "http://github.com/TheRealFooBar/Openlol",
	"license": "GNU GPL v3",
	"authors": [
		{
			"name": "The Real FooBar"
		}
	],
	"require": {
		"php": ">=7.0",
		"slim/slim": "^3.1",
		"slim/php-view": "^2.0",
		"monolog/monolog": "^1.17"
	},
	"require-dev": {
		"phpunit/phpunit": ">=5.0"
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"autoload": {
		"psr-4": {
			"Openlol\\": "src/openlol/"
		}
	},
	"config": {
		"process-timeout": 0,
		"sort-packages": true
	},
	"scripts": {
		"start": "php -S localhost:8080 -t public",
		"test": "phpunit"
	}
}
